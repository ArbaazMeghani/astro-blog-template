---
import Footer from '../components/Footer';
import Header from '../components/Header';
import Seo from '../components/Seo.astro';

export interface Props {
	title: string;
}

const { title } = Astro.props;
---

<!DOCTYPE html>
<html lang="en">
	<head>
		<Seo title={title} />
	</head>
	<body>
		<Header />
		<slot />
		<Footer />
	</body>
</html>